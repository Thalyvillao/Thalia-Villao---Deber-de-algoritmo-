//Dado un vector de n√∫meros determina aquellos que sea primos
Algoritmo nprimos
	Dimension vector[4]
	Escribir "Ingrese el tamano del vector"
	Leer ns
	Para i<-1 hasta ns Hacer
		Escribir "Introduzca el numero para el vector ",i
		Leer vector[i]
	FinPara
	Para i<-1 hasta ns Hacer
		cont=0
		Para j<-1 Hasta vector[i] Hacer
			si vector[i]%j=0 Entonces
				cont=cont+1
			FinSi
		FinPara
		Si cont=2 Entonces
			Escribir "El numero primo encontrado es: ",vector[i]
		FinSi
	FinPara
FinAlgoritmo
